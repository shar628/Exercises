  $(document).ready(function() {
    jQuery(function ($) {
        $(".navbar-nav.simplemenu a")
            .click(function(e) {
                var link = $(this);

                var item = link.parent("li");

                if (item.hasClass("activea")) {
                    item.removeClass("activea").children("a").removeClass("activea");
                } else {
                    item.addClass("activea").children("a").addClass("activea");
                }

                if (item.children(".navbar-nav.simplemenu").length > 0) {
                    var href = link.attr("href");
                    link.attr("href", "#");
                    setTimeout(function () { 
                        link.attr("href", href);
                    }, 300);
                    e.preventDefault();
                }
            })
            .each(function() {
                var link = $(this);
                if (link.get(0).href === location.href) {
                    link.addClass("activea").parents("li").addClass("activea");
                    return false;
                }
            });
    });
});
  
